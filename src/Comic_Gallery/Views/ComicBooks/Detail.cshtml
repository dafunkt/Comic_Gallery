    var description = "<p> FInal issue!</p>";
    var artists = new string[]
    {
        "Script: Dan Slott",
        "Pencils: Humberto",
        "Inks: Victor",
        "Colors: Edgar Delgado",
        "Letters: Chris Elio"
    };
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
    <h1>@SeriesTitle</h1>
    <h2>Issue @IssueNumber</h2>
    <h5>Decription</h5>
    <div>@Html.Raw(description)</div>
    <h5>Artists: </h5>
        <div>@artists</div>
</body>
</html>
